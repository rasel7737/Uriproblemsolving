
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double a,b;
		int n = sc.nextInt();
		for (int i=1;i<=n;i++)
		{
			a=sc.nextDouble();
			b=sc.nextDouble();
			if (b==0) {
				System.out.println("divisao impossivel");
			}
			else
			{
				System.out.println((double)(a/b));
			}
			
		}
	}

}
