
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();// taking user input 
		
		if (n>2 && n<1000) {  /// checking the user input is between 2 & 1000
			for (int i =1;i<=10;i++) {
				System.out.println(i+" x "+n+" = "+i*n); // printing the results of i x n
			}
		}

	}

}
