
import java.util.Scanner;
public class Main {
public static void main (String[] args) {
	Scanner sc = new Scanner(System.in); //creating scanner for user input
	int n,in=0,out=0;
	n=sc.nextInt(); //taking user input
	int [] arr = new int[n]; //size of arry = userinput
	for (int i=0;i<arr.length;i++) {
		
		arr[i]=sc.nextInt();
		if (arr[i]>=10 && arr[i]<=20)
		{
			in++;
			
		}
		else
			out++;
	}
	System.out.println(in+" in\n"+out+" out");  //printing result
}
}
