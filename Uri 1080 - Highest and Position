import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int highest =0,pos=0;
	   int x;
	   
	   for (int i=1;i<=100;i++) {
		   
		   x=sc.nextInt();
		   if(highest<x) {
			   highest=x;
			   pos=i;
		   }
		   
		   
	   }
	   System.out.println(highest+"\n"+pos);
	   
	}

	}


