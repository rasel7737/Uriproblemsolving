import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double n,sum=0,result=0;
		boolean loop = true;

		while(loop) {
			n=sc.nextDouble();
			if(n<0 || n>10)
				System.out.println("nota invalida");
			else {
				sum+=n;
				result++;
			}
			if(result==2)
				break;
		}
		System.out.printf("media = %.2f\n",sum/result);
	}

}
