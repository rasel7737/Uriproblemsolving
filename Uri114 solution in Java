
import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		boolean loop = true;
		while(loop) {
			if (a==2002) {
				System.out.println("Acesso Permitido");
				break;
			}
				
			else {
				
				System.out.println("Senha Invalida");
				a = sc.nextInt();
			}
		}
		// TODO Auto-generated method stub

	}

}
