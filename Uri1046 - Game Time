import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
	    int n1,n2,ft=0;
	    n1=sc.nextInt();
	    n2=sc.nextInt();
	    
	    
	    
	    if(n1==n2) {
	    	ft=24;
	    }
	    
	    
	    else if(n1>n2) {
	    	n2+=24;
	    	ft=n2-n1;
	    	
	    }
	    else
	    	ft=n2-n1;
	    
	    System.out.println("O JOGO DUROU "+ft+" HORA(S)");
	
	
	
	}

	}


