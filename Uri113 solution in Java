
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a,b;
		a=sc.nextInt();
		b=sc.nextInt();
		
		boolean loop = true;
		while (loop) {
			if (a==b)
				break;
			else {
				if (a>b)
					System.out.println("Decrescente");
				else
					System.out.println("Crescente");
					
				a=sc.nextInt();
				b=sc.nextInt();
			
		}

	}

}
}
